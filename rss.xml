<?xml version="1.0" encoding="utf-8" ?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">joshdmiller.com | Blog</title>
    <link type="application/atom+xml" href="http://joshdmiller.com/rss.xml" rel="self"/>
    <link type="text" href="http://joshdmiller.com" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://www.joshdmiller.com</id>
    <author>
        <name>Josh D Miller :: Blog</name>
    </author>
    <rights>Copyright (c) 2010-2012 Josh D Miller</rights>
    {% for post in site.posts limit:20 %}
    <entry>
        <title></title>
        <link href="http://www.joshdmiller.com{{ post.url }}/"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://www.joshdmiller.com{{ post.url }}/</id>
        <summary type="html">
            {% capture summary %}{{ post.content | summarize }}{% endcapture %}
            {% if summary != post.content %}
                {{ summary | xml_escape }}
                <p><a href="{{ post.url }}">Continue reading Â»</a></p>
            {% else %}
                {{ post.content | xml_escape }}
            {% endif %}
        </summary>
    </entry>
    {% endfor %}
</feed>
